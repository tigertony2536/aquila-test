{
  "extends": "./base.json",
  "compilerOptions": {
    "target": "nodenext",
    "useDefineForClassFields": true,
    "module": "nodenext",
    "lib": ["ESNext", "DOM"],
    "sourceMap": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck":true
  },
  "exclude": ["node_modules"]
}
